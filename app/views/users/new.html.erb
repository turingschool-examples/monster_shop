<h1>Create Profile</h1>
<hr>
<%= form_for @user, value: nil do |f| %>
	<%= f.label :name %>
  <%= f.text_field :name %>
	<br>
	<%= f.label :user_name %>
	<%= f.text_field :user_name%>
  <br>
  <%= f.label :password %>
  <%= f.password_field :password, value: nil%>
  <br>
  <%= f.label :password_confirmation %>
  <%= f.password_field :password_confirmation%>
    <br>
	<%= f.label :address %>
  <%= f.text_field :address %>
  <br>
	<%= f.label :city %>
  <%= f.text_field :city%>
  <br>
	<%= f.label :state %>
  <%= f.text_field :state %>
  <br>
	<%= f.label :zip%>
  <%= f.text_field :zip%>
  <br>
  <%= f.submit %>
<% end %>
